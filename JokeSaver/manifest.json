{
    "manifest_version": 3,
    "name": "Make Me Laugh",
    "version": "1.0",
    "description": "Displays a random joke from an external API when you click 'Make Me Laugh'.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/i1.png",
        "48": "icons/i2.png",
        "128": "icons/i3.png"
      }
    },
    "icons": {
      "16": "icons/i1.png",
      "48": "icons/i2.png",
      "128": "icons/i3.png"
    }
  }
  